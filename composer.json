{
  "name": "dong/xthk-logger",
  "description": "日志公共组件",
  "type": "library",
  "authors": [
    {
      "name": "tangbo",
      "email": "admin@tbphp.net"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": ">=5.6",
    "illuminate/http": ">=5.6",
    "monolog/monolog": "^1.23|^2.1.1",
    "moontoast/math": "^1.2",
    "ramsey/uuid": ">=3.0"
  },
  "autoload": {
    "psr-4": {
      "Xthk\\Logging\\": "./src"
    }
